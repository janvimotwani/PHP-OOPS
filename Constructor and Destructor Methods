class Car {
    public $brand;
    public $color;

    public function __construct($brand, $color) {
        $this->brand = $brand;
        $this->color = $color;
    }

    public function startEngine() {
        echo "Engine started!";
    }

    public function __destruct() {
        echo "Car object destroyed.";
    }
}

$myCar = new Car("Toyota", "Blue");
$myCar->startEngine(); 
